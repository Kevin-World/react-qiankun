@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@keyframes slideInUp {
  from {
    transform: translate3d(0, 100%, 0);
  }

  to {
    transform: translate3d(0, 0, 0);
  }
}
@keyframes slideInDown {
  from {
    transform: translate3d(0, -100%, 0);
  }

  to {
    transform: translate3d(0, 0, 0);
  }
}
.fade-enter-active {
  animation: fadeIn 0.3s cubic-bezier(0.36, 0.66, 0.04, 1);
}
.fade-leave-active {
  animation: fadeIn 0.3s cubic-bezier(0.36, 0.66, 0.04, 1) reverse;
}

.slide-in-up-enter-active {
  animation: slideInUp 0.2s cubic-bezier(0.36, 0.66, 0.04, 1);
}
.slide-in-up-leave-active {
  animation: slideInUp 0.2s cubic-bezier(0.36, 0.66, 0.04, 1) reverse;
}
.slide-up-down-enter , .slide-up-down-leave-to {
		transform: translateY(100%);
	}
.slide-up-down-enter-active , .slide-up-down-leave-active {
		transition: all .4s ease;
	}
.slide-in-down-enter-active {
  animation: slideInDown 0.2s cubic-bezier(0.36, 0.66, 0.04, 1);
}
.slide-in-down-leave-active {
  animation: slideInDown 0.2s cubic-bezier(0.36, 0.66, 0.04, 1) reverse;
}
.Router
  position absolute
